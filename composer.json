{
  "name": "doccheck/oauth2-doccheck",
  "description": "DocCheck OAuth 2.0 Client Provider for The PHP League OAuth2-Client",
  "license": "MIT",
  "authors": [
    {
      "name": "DocCheck IT",
      "email": "it@doccheck.com",
      "homepage": "https://www.doccheck.com"
    }
  ],
  "keywords": [
    "oauth",
    "oauth2",
    "client",
    "authorization",
    "authorisation",
    "doccheck"
  ],
  "require": {
    "php": ">=8.2",
    "league/oauth2-client": "^2.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^11.0",
    "symfony/var-dumper": "^7.2"
  },
  "autoload": {
    "psr-4": {
      "DocCheck\\OAuth2\\Client\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "DocCheck\\OAuth2\\Client\\Test\\": "tests/"
    }
  }
}
